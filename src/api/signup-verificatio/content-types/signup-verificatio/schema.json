    },
    "phoneNumber": {
      "type": "text",
      "required": true,
      "maxLength": 11
    },
    "phonecode": {
      "type": "biginteger",
      "max": "6"
    },
    "password": {
      "type": "text",
      "maxLength": 12,
      "minLength": 6
    },
    "isEmailVerified": {
      "type": "boolean"
    },
    "isPhoneVerified": {
      "type": "boolean",
      "default": false
    }
  }
